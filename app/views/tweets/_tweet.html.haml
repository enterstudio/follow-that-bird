.tweet{:id => tweet.element_id}
  .from_user= link_to_twitter_user tweet.from_user
  .text= tweet.text
  %button.vote.up{:style => "border-color: white; background-color: #090;"}
    = image_tag("thumb_up.png", :alt => "Vote up", :title => "Vote up")
  %button.vote.down{:style => "border-color: white; background-color: #900;"}
    = image_tag("thumb_down.png", :alt => "Vote down", :title => "Vote down")